# INPUT
fun test(n) {
  if n *= 2 {
    return "BING";
  } else {
    return "BONG";
  }
}
map main {
  b = root.attr + test(2) + test(1);
}

# CONST POOL
000: attr
001: BING
002: BONG

# OUTPUT
000: LOAD       0 = (root)
001: ATTR       0 = (attr)
002: CONST      1 = ('BING')
003: ADD        0
004: CONST      2 = ('BONG')
005: ADD        0
006: STORE      1 = (b)
